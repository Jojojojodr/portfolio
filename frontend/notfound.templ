package frontend

import (
    "github.com/gin-gonic/gin"
)

templ NotFound(c *gin.Context, context string) {
    @Layout(c, "Not Found") {
        <div class="flex flex-col items-center justify-center">
            <h1 class="pb-3 text-2xl">404 Page Not Found</h1>
            <p>{ context }</p>
        </div>
    }
}